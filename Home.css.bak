
body {
	background-color: #98ACB5;
	height:100vh;
	justify-content: center;
	background-image: url("Door.jpg");
	background-size: fit;
	background-attachment: fixed;
	min-height: 100%;
	background-position: center;
	font-family: "Poppins";
	margin: 0;
	box-sizing: border-box;
}
.titleStyle{
	font-family: 'Alkatra', cursive;
}
.container{
	width: 90%;
	height:380px;
}
.carousel-inner {
	width: fit;
	height:380px;
  }
*, *:before, *:after {
	box-sizing: inherit;
}
.topnav .search-container {
	float: right;
}
.topnav .search-container button {
	float: right;
	padding: 6px 10px;
	margin-top: 8px;
	margin-right: 16px;
	background: #ddd;
	font-size: 17px;
	border: none;
	cursor: pointer;
}

.head-section {
	padding: 20px;
	text-align: left;
	background-color: #474e5d;
	color: white;
}

.card {
	box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2);
	margin: 5px;
	border-radius: 0.5em 0.5em 0.5em 0.5em;
}
.Event {
	padding: 10px 5px;
}
.item{
	left: 30% !important;
	right: 30% !important;
}
.activities{
	padding: 0px 25px;
}
.row {
	padding: 20px 10px;
}
.containerc {
	background-color: white;
	padding: 30px 30px;
	box-shadow: 0 4px 8px 8px 096292;
	border-radius: 0.5em 0.5em 0.5em 0.5em;
}

.title {
  color: grey;
}

.column {
	float: left;
	width: 33.3%;
	margin-bottom: 15px;
	padding: 0 15px;
}
.containerc::after, .row::after {
	content: "";
	clear: both;
	display: table;
}


.button {
	border: none;
	outline: 0;
	display: inline-block;
	padding: 8px;
	color: white;
	background-color: #096292;
	text-align: center;
	cursor: pointer;
	width: 100%;
}
.up{color: white;}

.button:hover {
	background-color: #555;
}

h1	{
	padding: 20px 10px;
	text-align: center;
	color:#096292;
	font-size:50px;
}
.info {
	padding: 20px;
	background-color: #474e5d;
	color: white;

}
.text{
	font-size:15px;
	padding: 30px;
	background-color: #474e5d;
	color: white;


}





